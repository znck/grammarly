{
  "name": "grammarly",
  "publisher": "znck",
  "displayName": "Grammarly",
  "description": "Grammarly grammar checking for Visual Studio Code.",
  "version": "0.1.6",
  "icon": "assets/logo.png",
  "engines": {
    "vscode": "^1.39.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/znck/grammarly"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:plaintext",
    "onLanguage:markdown",
    "onCommand:grammarly.check"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Grammarly",
      "properties": {
        "grammarly.username": {
          "description": "A username for grammarly.com (optional).",
          "scope": "resource",
          "type": "string"
        },
        "grammarly.password": {
          "description": "A password for grammarly.com (optional).",
          "scope": "resource",
          "type": "string"
        },
        "grammarly.dialect": {
          "description": "The default dialect of English language.",
          "scope": "resource",
          "enum": [
            "american",
            "british"
          ],
          "default": "american"
        }
      }
    },
    "commands": [
      {
        "command": "grammarly.check",
        "title": "Check grammar errors",
        "category": "Grammarly",
        "enablement": "editorLangId == markdown || editorLangId == plaintext"
      }
    ]
  },
  "license": "MIT",
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "test-compile": "tsc -p ./",
    "compile": "rollup -c",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "@types/node-fetch": "^2.5.3",
    "@types/turndown": "^5.0.0",
    "@types/ws": "^6.0.3",
    "rollup": "^1.26.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-typescript2": "^0.24.3",
    "tslib": "^1.10.0",
    "typescript": "^3.3.1",
    "vscode": "^1.1.28"
  },
  "dependencies": {
    "bufferutil": "^4.0.1",
    "debug": "^4.1.1",
    "html-to-md": "^0.3.2",
    "node-fetch": "^2.6.0",
    "supports-color": "^7.1.0",
    "utf-8-validate": "^5.0.2",
    "vscode-languageclient": "6.0.0-next.1",
    "vscode-languageserver": "6.0.0-next.1",
    "vscode-languageserver-protocol": "^3.14.1",
    "ws": "^7.2.0"
  }
}
