{
  "name": "grammarly-language-server-transformers",
  "types": "src/index.d.ts",
  "browser": "./dist/index.browser.js",
  "module": "./dist/index.node.js",
  "main": "./dist/index.node.cjs",
  "publishConfig": {
    "types": "./dist/index.d.ts"
  },
  "buildConfig": {
    "useMain": false,
    "sources": {
      "src/index.d.ts": [
        {
          "format": "dts",
          "file": "dist/index.d.ts"
        }
      ],
      "src/index.browser.ts": [
        {
          "format": "esm",
          "file": "dist/index.browser.js"
        }
      ],
      "src/index.node.ts": [
        {
          "format": "esm",
          "file": "dist/index.node.js"
        },
        {
          "format": "cjs",
          "file": "dist/index.node.cjs"
        }
      ]
    },
    "external": [
      "tree-sitter-wasm-prebuilt/lib/tree-sitter-html.wasm",
      "tree-sitter-wasm-prebuilt/lib/tree-sitter-markdown.wasm"
    ]
  },
  "exports": {
    "node": {
      "import": "./dist/index.node.js",
      "require": "./dist/index.node.cjs"
    },
    "browser": {
      "import": "./dist/index.browser.js"
    },
    "default": {
      "import": "./dist/index.browser.js"
    }
  },
  "dependencies": {
    "tree-sitter": "^0.20.0",
    "tree-sitter-html": "^0.19.0",
    "tree-sitter-markdown": "^0.7.1",
    "web-tree-sitter": "^0.20.5",
    "tree-sitter-wasm-prebuilt": "^0.0.3"
  },
  "devDependencies": {
    "@grammarly/sdk": "^1.7.2"
  }
}
