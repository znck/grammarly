{
  "files.exclude": {
    "out": false
  },
  "search.exclude": {
    "out": true
  },
  "typescript.tsc.autoDetect": "off",
  "editor.formatOnSave": true,
  "cSpell.words": [
    "AUTOCORRECT",
    "capi",
    "docid",
    "Emogenie",
    "errored",
    "freews",
    "gnar",
    "Grammarly",
    "heatmap",
    "inversify",
    "languageclient",
    "minicard",
    "reqid",
    "subalerts",
    "textdocument"
  ],
  "typescript.preferences.quoteStyle": "single",
  "typescript.preferences.importModuleSpecifierEnding": "minimal",
  "cSpell.enabled": true
}
